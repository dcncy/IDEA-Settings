<templateSet group="Dcncy">
  <template name="*" value="*&#10; *&#10; * 功能描述: $end$&#10; *&#10; * @param:  $param$&#10; * @return: $return$&#10; * @author: jisiqi&#10; * @date:   $date$ $time$&#10; */" shortcut="TAB" description="默认注释" toReformat="false" toShortenFQNames="true">
    <variable name="end" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="param" expression="methodParameters()" defaultValue="" alwaysStopAt="true" />
    <variable name="return" expression="methodReturnType()" defaultValue="" alwaysStopAt="true" />
    <variable name="date" expression="date()" defaultValue="" alwaysStopAt="true" />
    <variable name="time" expression="time()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="forthread" value="for(int i = 0; i &lt; 10; i++){&#10;    new Thread(()-&gt;{&#10;        $start$&#10;    }, String.valueOf(i)).start();&#10;}" description="自定义多线程运行" toReformat="false" toShortenFQNames="true">
    <variable name="start" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="mainsp" value="public static void main(String[] args) {&#10;    SpringApplication.run($className$.class, args);  &#10;}" description="springboot main方法" toReformat="false" toShortenFQNames="true">
    <variable name="className" expression="className()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="psfl" value="private static final Logger LOG = LoggerFactory.getLogger($fileName$.class);" description="log变量定义" toReformat="false" toShortenFQNames="true">
    <variable name="fileName" expression="fileNameWithoutExtension()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="newth" value="new Thread(()-&gt;{&#10;    $start$&#10;}, &quot;$start$&quot;).start();" description="新建线程" toReformat="false" toShortenFQNames="true">
    <variable name="start" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="newlock" value="Lock lock = new ReentrantLock();&#10;lock.lock();&#10;try {&#10;    $start$&#10;}catch (Exception e){&#10;    e.printStackTrace();&#10;}finally {&#10;    lock.unlock();&#10;}" description="创建个锁" toReformat="false" toShortenFQNames="true">
    <variable name="start" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="thsleep" value="try {TimeUnit.SECONDS.sleep($start$);} catch (InterruptedException e) {e.printStackTrace();}" description="线程睡眠几秒" toReformat="false" toShortenFQNames="true">
    <variable name="start" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
  <template name="soup" value="System.out.println($start$);" description="打印" toReformat="false" toShortenFQNames="true">
    <variable name="start" expression="" defaultValue="" alwaysStopAt="true" />
  </template>
  <template name="trylock" value="Lock lock = new ReentrantLock();&#10;lock.lock();&#10;try {&#10;&#10;} catch (Exception e) {&#10;    e.printStackTrace();&#10;} finally {&#10;    lock.unlock();&#10;}" description="lock锁使用" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>